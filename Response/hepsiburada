                        <!-- ko foreach: getMoreItems('Brand') -->
                        <li data-bind="attr : {title: $data.name, alt: $data.name}, css: {'selected': $data.selected, 'unselected': !$data.selected }">
                            <input type="checkbox"
                                   class="checkbox"
                                   name="filter-brand"
                                   data-bind="checked: $parent.appliedFilter('brand', $data.id), click: $parent.toggleFilter.bind($parent, 'brand', $data.id), value: $data.id, attr: {id: 'brand-' + $data.id}" />
                            <label data-bind="attr : {for: 'brand-'+$data.id}">
                                <span class="filter-text" data-bind="text: $data.name"></span>
                                <span data-bind="text : '('+$data.itemsCount+')'"></span>
                            </label>
                        </li>
                        <!-- /ko -->
