<script type="text/html" id="menuItems">
    <!-- ko if: $data.title != "Sanal Grup" -->
    <li data-bind="attr: {'data-submenu-id': $data.title != 'Sanal Grup' ? 'menu-'+ $index() : ''}">
        <a href="javascript:;" class="menu-item" data-bind="click: $root.menuItemClicked, text: $data.title, attr: {href: $data.url, 'data-title': $data.title, 'data-path': '/menu', 'data-categoryid': $data.categoryId}"></a>

        <!-- ko if: $data.children.length > 0 -->
        <ul data-bind="template: { name: 'menuItems', foreach: children}, attr: {id: $data.title != 'Sanal Grup' ? 'menu-'+ $index() : ''}"></ul>
        <!-- /ko -->
        <!-- ko if: $data.banners.length > 0 -->
        <div class="flyout" data-bind="foreach: banners">
            <a href="javascript:;" data-bind="attr: {'href' : $data.link, 'data-id' : $data.id, 'data-sortid' : $data.sortId, 'data-path': $data.name ,'data-name': 'menu/'}">
                <img data-bind="attr: {'src' : $data.mediaPath}" />
            </a>
        </div>
        <!-- /ko -->
    </li>
    <!-- /ko -->
    <!-- ko if: $data.title == "Sanal Grup" -->
    <ul class="children" data-bind="template: { name: 'menuItems', foreach: children}"></ul>
    <!-- /ko -->
</script>

