	<!--SUGGESTION BUTTON-->

	<div id="fixedbutton">
	    <a id="top-button">
		<span class="tooltip sprite sprite-button_top_h"  title="En üste çık" style="border-style: none " id="top-button-image">
	    </a>
	    <a  id="suggest-button" onclick="showAjaxBox('https://www.kitapyurdu.com/index.php?route=information/feedback/popup');" style="margin-bottom:2px; border-style: none; ">
		<span class="tooltip sprite sprite-button_suggest" title="Önerim Var" alt="Öneride bulun" id="suggest-image">

	    </a>
	    <a id="down-button">
		<span class="tooltip sprite sprite-button_down_h"  title="En alta in" style="border-style: none " id="down-button-image">
	    </a>
	</div>

	<script type="text/javascript">

	    $(document).ready(function () {


		$('#down-button').show();
		$('#top-button').hide();
	    });


	    $(document).ready(function () {
		$('#down-button').click(function () {

		    $('html, body').animate({scrollTop: $(document).height()}, 'slow');
		    $('#down-button').hide();
		    $('#top-button').show();
		    return false;

		});

	    });



	    $(document).ready(function () {
		$('#top-button').click(function () {

		    $('html, body').animate({scrollTop: 0}, 'slow');
		    $('#top-button').hide();
		    $('#down-button').show();
		    return false;
		});
	    });
	    $(document).ready(function () {
		$('#suggest-button').mouseover(function () {
		    jQuery('#suggest-image').addClass("sprite-button_suggest_h");
		    jQuery('#suggest-image').removeClass("sprite-button_suggest");

		}).mouseout(function () {
		    jQuery('#suggest-image').addClass("sprite-button_suggest");
		    jQuery('#suggest-image').removeClass("sprite-button_suggest_h");
		});
	    });
	    $(window).scroll(function () {

		if ($(window).scrollTop() + $(window).height() > $(document).height() - 100) {
		    $('#down-button').hide();
		    $('#top-button').show();
		} else if ($(window).scrollTop() + $(window).height() > 100) {

		    $('#down-button').show();
		    $('#top-button').show();
		}

		if ($(window).scrollTop() == 0) {
		    $('#down-button').show();
		    $('#top-button').hide();
		}

	    });



	</script>
