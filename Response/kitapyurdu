		<script type="text/javascript">

		    showMiniFilter(false);
		    function showMiniKeyboard() {
			$('#virtual-key-container').toggle();
			$('#virtual-detail-filter-container').hide();
		    }

		    $('#virtual-key-container button').unbind('click').click(function () {
			var letter = $(this).html();
			var $input = $('#search input[name=search_keyword]');
			var val = $input.val();
			if (val == 'kitap adı, yazar veya yayınevi ara') {
			    val = '';
			}
			$input.val(val + letter);
			$input.keyup();
		    });


		    function showMiniFilter(openToggle) {
			if (openToggle) {
			    $('#virtual-detail-filter-container').toggle();
			    $('#virtual-key-container').hide();
			}
			if ($('#detail-filter-publisher').is(':checked'))
			{
			    $("#filter-detail-button").css("color", "#63c831");
			} else if ($('#detail-filter-points-catalog').is(':checked'))
			{
			    $("#filter-detail-button").css("color", "#63c831");
			} else if ($('#detail-filter-category').is(':checked'))
			{
			    $("#filter-detail-button").css("color", "#63c833");
			} else {
			    $("#filter-detail-button").css("color", "black");
			}
		    }

		</script>

